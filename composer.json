{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "^8.3",
        "composer/installers": "^2.3",
        "cweagans/composer-patches": "^2.0",
        "drupal/address": "^2.0",
        "drupal/admin_toolbar": "^3.6",
        "drupal/block_field": "^1.0@RC",
        "drupal/color": "^2.0@alpha",
        "drupal/color_field": "^3.0",
        "drupal/core-composer-scaffold": "^11.2",
        "drupal/core-recipe-unpack": "^11.3",
        "drupal/core-recommended": "^11.2",
        "drupal/disable_language": "^1.0@RC",
        "drupal/entity_browser": "^2.15",
        "drupal/facets": "^3.0",
        "drupal/file_mdm": "^3.2",
        "drupal/gin": "^5.0",
        "drupal/gin_toolbar": "^3.0",
        "drupal/honeypot": "^2.2",
        "drupal/maxlength": "^3.1",
        "drupal/metatag": "^2.2",
        "drupal/oembed_providers": "^2.2",
        "drupal/paragraphs": "^1.19",
        "drupal/pathauto": "^1.14",
        "drupal/redirect": "^1.12",
        "drupal/redirect_after_login": "^3.0",
        "drupal/redis": "^1.11",
        "drupal/search_api": "^1.40",
        "drupal/token": "^1.16",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/viewsreference": "^2.0@beta",
        "drush/drush": "^13.7",
        "platformsh/config-reader": "^3.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-recipe-unpack": true
        }
    },
    "autoload": {
        "files": [
            "load.environment.php"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "recipes/{$name}": ["type:drupal-recipe"]
        },
        "enable-patching": true,
        "patches": {
        }
    }
}
